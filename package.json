    "gulp-uglify": "^2.0.0",
    "gulp-useref": "^3.1.2",
    "gulp-util": "^3.0.7",
    "jeet": "^6.1.5",
    "node-bourbon": "^4.2.8",
    "node-notifier": "^4.5.0",
    "node-sass": "^3.7.0",
    "nyc": "^10.1.2",
    "react-addons-test-utils": "^15.4.2",
    "require-globify": "^1.3.0",
    "sinon": "^1.17.7",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "chartjs": "^0.3.24",
