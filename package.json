    "browserify": "^13.1.0",
    "collecticons-processor": "^3.0.0",
    "del": "^2.2.2",
    "envify": "^3.4.1",
    "enzyme": "^2.7.1",
    "eslint": "3.6.1",
    "eslint-config-standard": "6.2.0",
    "eslint-plugin-promise": "2.0.1",
    "eslint-plugin-react": "6.3.0",
    "eslint-plugin-standard": "2.0.1",
    "faker": "^4.1.0",
    "gulp": "^3.9.1",
    "gulp-cache": "^0.4.5",
    "gulp-csso": "^2.0.0",
    "gulp-exit": "0.0.2",
    "gulp-if": "^2.0.1",
