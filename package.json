  "scripts": {
    "postinstall": "[ -f app/assets/scripts/config/local.js ] || echo 'module.exports = {}' > app/assets/scripts/config/local.js",
    "serve": "node ./.build_scripts/placeholder-articles.js && node ./.build_scripts/article-metadata.js && gulp collecticons && DS_ENV=development NODE_ENV=development gulp serve",
    "build": "node ./.build_scripts/placeholder-articles.js && node ./.build_scripts/article-metadata.js && gulp collecticons && DS_ENV=production NODE_ENV=production gulp",
    "lint": "eslint app/assets/scripts/ --ext .js",
    "test": "DS_ENV=testing nyc ava test -c 4",
    "report": "nyc report"
  },
  "ava": {
    "require": [
      "babel-core/register"
    ],
    "babel": {
      "presets": [
        "es2015",
        "react"
      ]
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "ignore": "test",
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ],
      "require-globify",
      "envify"
    ]
  },
  "devDependencies": {
    "ava": "^0.18.2",
